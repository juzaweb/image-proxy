{
    "name": "juzaweb/image-proxy",
    "homepage": "https://juzaweb.com",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "Image url proxy for posts",
    "extra": {
        "juzaweb": {
            "name": "Image Proxy",
            "domain": "juim",
            "cms_min": "3.3",
            "version": "1.0",
            "providers": [
                "Juzaweb\\ImageProxy\\Providers\\ImageProxyServiceProvider"
            ]
        }
    },
    "require-dev": {
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\ImageProxy\\": "src/"
        }
    }
}
